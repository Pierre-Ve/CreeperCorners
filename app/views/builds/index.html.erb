<div class="container">
  <h1 class="mt-4">Discover the best Minecraft Builds</h1>
  <div class=" pb-5">
      <%= render 'filter', build: @build %>
  </div>
</div>

  <div class="d-flex flex-wrap container">
    <% @builds.each do |build| %>
      <%= render build, build: build, lien: build_path(build), name: "View details" %>
    <% end %>
  </div>
</div>
